    <div class="ui inverted pointing menu">
      <a class="item" href="/">
        <!-- <img id="logo" src="https://www.48hourslogo.com/48hourslogo_data/2016/12/21/56696_1482330752.png"> -->
          <i class="podcast icon blue ml"></i>
        TechShare
      </a>
      <a class="active item" href="/">
        Home
      </a>
      <a class="item" href="/content">
        All
      </a>
      <a class="item" href="/content/new">
        New
      </a>
      <div class="right menu">
        <% if(!currentUser){ %>
          <a class="ui item" href="/register">
            SignUp
          </a>
          <a class="ui item" href="/login">
            Login
          </a>
        <% }else{ %>
          <a class="ui item" href="#">
            Signed in as <%= currentUser.username %>
          </a>
          <a class="ui item" href="/logout">
            Logout
          </a>
        <% } %>
      </div>
  </div>


